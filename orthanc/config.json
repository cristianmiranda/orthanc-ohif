{
    "Name": "Orthanc Server in Docker",
    "RemoteAccessAllowed": true,
    "AuthenticationEnabled": false,
    "Plugins": [
        "/usr/share/orthanc/plugins/"
    ],
    "Authorization": {
        "WebServiceRootUrl": "http://authenticator:8200/",
        "TokenHttpHeaders": [
            "Authorization",
            "Referer",
            "token"
        ],
        "TokenGetArguments": [
            "token"
        ]
    }
}