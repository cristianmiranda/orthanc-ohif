{
    "Name": "Orthanc Server in Docker",
    "RemoteAccessAllowed": true,
    "AuthenticationEnabled": false,
    "Plugins": [
        "/usr/share/orthanc/plugins/"
    ],
    "Authorization": {
        "WebServiceTokenValidationUrl": "http://authenticator:8200/tokens/validate",
        "TokenHttpHeaders": [
            "Authorization",
            "Referer",
            "token"
        ],
        "TokenGetArguments": [
            "token"
        ],
        "UncheckedFolders": [
            "/app/libs/images/"
        ]
    }
}
